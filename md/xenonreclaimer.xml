			<cues>
				<cue name="Emperor_Pride" instantiate="false">
					<conditions>
						<event_object_changed_owner group="$XenonCore"/>
						<check_value value="event.param2 == faction.xenon"/>
						<check_value value="event.object.macro == macro.Cluster_424_Sector002_macro"/>
					</conditions>
					<actions>
						<set_job_active job="'xenon_reclaimer_emperor_pride'" activate="true"/>
					</actions>
				</cue>
