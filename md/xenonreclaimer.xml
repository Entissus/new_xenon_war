<?xml version="1.0" encoding="utf-8"?>
<mdscript name="xenonreclaimer" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="md.xsd">
	<cues>
		<cue name="Init">
			<actions>
				<find_sector groupname="$XenonCore" multiple="true" space="player.galaxy" macro="[macro.Cluster_424_Sector002_macro,macro.Cluster_415_Sector001_macro,macro.Cluster_33_Sector001_macro,macro.Cluster_17_Sector001_macro,macro.Cluster_26_Sector001_macro,macro.Cluster_21_Sector001_macro,macro.Cluster_112_Sector001_macro"/>
